{
    "name": "j4k/api",
    "description": "API Response / Serializer Package for Laravel",
    "keywords": [
        "j4k",
        "api"
    ],
    "homepage": "https://github.com/j4k/api",
    "license": "MIT",
    "authors": [
        {
            "name": "j4k",
            "email": "jw@jack.gd",
            "homepage": "https://j.ackwall.io",
            "role": "Devloper"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "league/fractal": "0.12.*",
        "hashids/hashids": "1.0.*",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery" : "~0.9",
        "lucadegasperi/oauth2-server-laravel" : "4.0.x@dev",
        "tymon/jwt-auth" : "0.5.*",
        "illuminate/routing" : "~5.0",
        "illuminate/events" : "~5.0",
        "illuminate/auth" : "~5.0",
        "illuminate/database" : "~5.0",
        "illuminate/pagination" : "~5.0",
        "illuminate/container" : "~5.0",
        "illuminate/contracts" : "~5.0",
        "illuminate/filesystem" : "~5.0",
        "squizlabs/php_codesniffer" : ">=2.3.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "j4k\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "j4k\\Api\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
